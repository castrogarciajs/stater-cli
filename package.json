{
  "name": "new-app",
  "type": "module",
  "version": "0.2.0",
  "packageManager": "pnpm@8.15.4",
  "description": "Starter cli is a command line to make boilerplate with your favorite,tools",
  "author": {
    "name": "Sebastián García",
    "email": "castrogarcia.mjs@gmail.com",
    "url": "https://castrogarciajs.dev"
  },
  "license": "MIT",
  "funding": "https://github.com/castrogarciajs/starter-cli",
  "homepage": "https://github.com/castrogarciajs/stater-cli/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/castrogarciajs/starter-cli",
    "directory": "castrogarciajs/starter-cli"
  },
  "bugs": {
    "url": "https://github.com/unocss/unocss/issues"
  },
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "new-app": "./bin/new-app.mjs"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "dist",
    "package.json",
    "resources"
  ],
  "scripts": {
    "build": "unbuild",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "dev": "unbuild --stub",
    "prepare": "husky",
    "deploy": "changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.4",
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "19.0.1",
    "@commitlint/config-conventional": "19.0.0",
    "@swc/core": "1.4.2",
    "@swc/jest": "0.2.36",
    "@types/jest": "29.5.12",
    "@types/minimist": "1.2.5",
    "@types/node": "20.11.21",
    "@types/prompts": "2.4.9",
    "cac": "6.7.14",
    "consola": "3.2.3",
    "eslint": "8.57.0",
    "eslint-plugin-format": "0.1.0",
    "esno": "4.0.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "lint-staged": "15.2.2",
    "minimist": "1.2.8",
    "prompts": "2.4.2",
    "typescript": "5.3.3",
    "unbuild": "2.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "eslint . --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "verbose": true
  }
}
